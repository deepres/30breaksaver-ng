{
  "name": "30min break saver",
  "background": {
    "page": "background.html",
    "persistent": false
  },
	"page_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [ "declarativeContent", "activeTab" ]

}
